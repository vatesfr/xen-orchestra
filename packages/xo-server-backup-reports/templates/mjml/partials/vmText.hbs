{{#if vm}}
<h3>{{vm.name_label}}</h3>
{{#if vm.name_description}}
{{vm.name_description}}
{{/if}}
<ul>
  <li><span style="font-weight: bold">UUID</span>: {{vm.uuid}}</li>
{{else}}
<h3>VM not found</h3>
<ul>
  <li><span style="font-weight: bold">UUID</span>: {{taskLog.data.id}}</li>
{{/if}}
{{>reportTemporalDataMjml end=taskLog.end start=taskLog.start}}
{{>reportWarningsMjml task=taskLog}}
{{>reportInfo task=taskLog}}
