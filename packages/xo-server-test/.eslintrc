{
  "parserOptions": {
    "ecmaVersion": 2021,  // Compatible avec ES12+
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.js", "*.{integ,spec,test}.{js,jsx}"],
      "rules": {
        "new-cap": ["warn"],  // Fix possible typo in rule
        "no-console": "warn",
        "n/no-unsupported-features/es-builtins": ["warn"],
      "n/no-unsupported-features/es-syntax": ["warn"]
      }
    }
  ]
}
