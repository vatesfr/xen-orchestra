    "micromatch": "^4.0.2",
    "mkdirp": "^1.0.4",
    "pretty-ms": "^7.0.0",
    "progress-stream": "^2.0.0",
    "promise-toolbox": "^0.20.0",
    "pump": "^3.0.0",
